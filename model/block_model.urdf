<?xml version="1.0" ?>
<robot name="myrobot">
    <link name="world" />
    
    <link name="base_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.06" iyz="0" izz="0.03"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="1 1 1"/>
      </geometry>
      <material name="red">
        <color rgba=".9 .1 .1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="1 1 1"/>
      </geometry>
      <material name="Grey"/>
    </collision>    
  </link>
  <joint name="joint00" type="fixed">
    <parent link="world"/>
    <child link="base_link"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
    <axis xyz="0 0 0"/>
    <limit effort="150" lower="-300000" upper="3000000" velocity="150000000"/>    
    <!--dynamics damping="3.0"/-->
  </joint> 

  <link name="front_left_coxa">
    <inertial>
      <origin rpy="0 0 0" xyz="0.5 0 0"/>
      <mass value="1"/>
      <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.06" iyz="0" izz="0.03"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0.5 0 0"/>
      <geometry>
        <box size="1 0.15 0.15"/>
      </geometry>
      <material name="red">
        <color rgba=".9 .1 .1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.5 0 0"/>
      <geometry>
        <box size="1 0.15 0.15"/>
      </geometry>
      <material name="Grey"/>
    </collision>    
  </link>

  <joint name="joint01" type="revolute">
    <parent link="base_link"/>
    <child link="front_left_coxa"/>
    <origin rpy="0 0 0" xyz="0.5 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="150" lower="-300000" upper="3000000" velocity="150000000"/>    
    <dynamics damping="2.0" friction="1.0"/>
  </joint>
  
  <link name="front_left_femour">
